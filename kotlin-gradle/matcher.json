{
  "problemMatcher": [
    {
      "severity": "warning",
      "owner": "kotlin-gradle-compiler-warning",
      "pattern": [
        {
          "regexp": "^w:\\sfile://(.+):(\\d+):(\\d+)\\s(.+)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "message": 4
        }
      ]
    },
    {
      "severity": "error",
      "owner": "kotlin-gradle-compiler-error",
      "pattern": [
        {
          "regexp": "^e:\\sfile://(.+):(\\d+):(\\d+)\\s(.+)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "message": 4
        }
      ]
    },
    {
      "severity": "warning",
      "owner": "kotlin-gradle-old-compiler-warning",
      "pattern": [
        {
          "regexp": "^w:\\s(.+):\\s\\((\\d+),\\s(\\d+)\\):\\s(.+)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "message": 4
        }
      ]
    },
    {
      "severity": "error",
      "owner": "kotlin-gradle-old-compiler-error",
      "pattern": [
        {
          "regexp": "^e:\\s(.+):\\s\\((\\d+),\\s(\\d+)\\):\\s(.+)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "message": 4
        }
      ]
    }
  ]
}
